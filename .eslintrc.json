{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/warnings",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console": "off",
        "new-cap": [
            "error",
            {
                "newIsCapExceptions": ["Buffer.from"]
            }
        ]
    }
}
